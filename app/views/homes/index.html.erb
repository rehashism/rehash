<%= render "refills/hero" %>
<%= render "refills/cards" %>
<%= render "refills/device" %>
<% if signed_in? %>
  <% if current_user.store.menus.nil? %>
    <%= link_to "Generate Page", homes_path(type: "first"), method: :post %>
  <% else %>
    <%= link_to "Refresh Page", homes_path(type: "refresh"), method: :post %>
  <% end %>
<% else %>
  <%= button_to "Sign up", sign_up_path, method: :get %>
<% end %>
